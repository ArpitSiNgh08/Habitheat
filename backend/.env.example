# Move to .env.local for local development
# Local development environment variables
PORT=5000
MONGODB_URI=your_mongodb_uri
JWT_SECRET_KEY=your_jwt_secret
DB_NAME=your_db_name
# For production, set CLIENT_ORIGIN to your deployed frontend URL, e.g. https://your-frontend-domain.com
CLIENT_ORIGIN=http://localhost:5173
# Set to 'production' in production, 'development' locally
NODE_ENV=development
